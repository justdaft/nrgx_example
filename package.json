{
  "name": "angular2-store-example",
  "version": "0.0.1",
  "description": "WIP: @ngrx/store Best Practices",
  "license": "MIT",
  "scripts": {
    "clean": "./node_modules/.bin/rimraf node_modules doc typings coverage dist && npm cache clean",
    "clean:dist": "./node_modules/.bin/rimraf dist",
    "clean:install": "npm run clean && npm install",
    "clean:start": "npm run clean && npm start",
    "watch": "npm run watch:dev",
    "watch:dev": "./node_modules/.bin/webpack --watch --progress --profile --colors --display-error-details --display-cached",
    "watch:prod": "./node_modules/.bin/webpack --watch --config webpack.prod.config.js --progress --profile --colors --display-error-details --display-cached",
    "build": "npm run build:dev",
    "build:dev": "npm run clean:dist && ./node_modules/.bin/webpack --progress --profile --colors --display-error-details --display-cached",
    "build:prod": "npm run clean:dist && ./node_modules/.bin/webpack --config webpack.prod.config.js --progress --profile --colors --display-error-details --display-cached",
    "server": "./node_modules/.bin/npm-run-all --parallel server:dev json-server",
    "json-server": "./node_modules/.bin/json-server ${JSON_SERVER_DB:=db.json} -q --port ${JSON_SERVER_PORT:=3100} -d ${JSON_SERVER_DELAY:=100}",
    "server:dev": "./node_modules/.bin/webpack-dev-server --progress --profile --colors --display-error-details --display-cached",
    "server:prod": "./node_modules/.bin/http-server dist --cors",
    "webdriver:update": "./node_modules/.bin/webdriver-manager update",
    "webdriver:start": "./node_modules/.bin/webdriver-manager start",
    "lint": "./node_modules/.bin/tsconfig-lint",
    "e2e": "./node_modules/.bin/protractor",
    "test": "./node_modules/.bin/karma start",
    "ci": "npm run e2e && npm run test",
    "docs": "./node_modules/.bin/typedoc  --options typedoc.json  src/**/*.ts",
    "start": "npm run server",
    "postinstall": "./node_modules/.bin/typings install && node setup-json-db.js"
  },
  "dependencies": {
    "@ngrx/store": "^1.2.0",
    "angular2": "2.0.0-beta.0",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.33.3",
    "es7-reflect-metadata": "^1.4.0",
    "immutable": "^3.7.6",
    "normalizr": "^1.4.1",
    "rxjs": "5.0.0-beta.0",
    "zone.js": "0.5.10"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^0.3.3",
    "css-loader": "^0.23.0",
    "exports-loader": "0.6.2",
    "expose-loader": "^0.7.1",
    "file-loader": "^0.8.4",
    "html-webpack-plugin": "^1.7.0",
    "http-server": "^0.8.5",
    "imports-loader": "^0.6.4",
    "istanbul-instrumenter-loader": "^0.1.3",
    "json-loader": "^0.5.3",
    "json-server": "^0.8.6",
    "karma": "^0.13.15",
    "karma-chrome-launcher": "^0.2.1",
    "karma-coverage": "^0.5.3",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-webpack": "1.7.0",
    "npm-run-all": "^1.4.0",
    "phantomjs": "^1.9.18",
    "phantomjs-polyfill": "0.0.1",
    "protractor": "^3.0.0",
    "raw-loader": "0.5.1",
    "reflect-metadata": "0.1.2",
    "remap-istanbul": "^0.5.1",
    "rimraf": "^2.4.4",
    "style-loader": "^0.13.0",
    "ts-helper": "0.0.1",
    "ts-loader": "^0.7.2",
    "tsconfig-lint": "^0.2.0",
    "tslint": "^3.2.0",
    "tslint-loader": "^2.1.0",
    "typedoc": "^0.3.12",
    "typescript": "^1.7.3",
    "typings": "^0.4.1",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.12.1",
    "webpack-md5-hash": "0.0.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/meenie/angular2-store-example.git"
  },
  "bugs": {
    "url": "https://github.com/meenie/angular2-store-example/issues"
  },
  "engines": {
    "node": ">= 4.2.1 <= 5",
    "npm": ">= 3"
  }
}
